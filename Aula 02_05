'''
x=int(input("Digite o ínicio: "))
y=int(input("Digite o fim: "))
print("Sequência de númeors inteiros:")
ct=0
soma=0
if(x<y):
    for number in range(x, y+1):
        print(number, end=" ")
        ct=ct+1
        soma=soma+number
    print("\nQuantidade de números",ct)
    print("Soma dos numeros: ",soma)
elif(x>y):
    for number in range(x, y-1, -1):
        print(number, end=" ")
        ct=ct+1
        soma=soma+number    
    print("\nQuantidade de números",ct)
    print("Soma dos numeros: ",soma)
else:
    print(x)
