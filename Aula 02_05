'''
x=int(input("Digite o valor inicial: "))
y=int(input("Digite o valor final: "))
print("Fahreinheit  │celsius")
soma=0

for f in range(x,y+1):
    c=5/9*(f-32)
    print(f"{f:.1f}ºF         │{c:.3f}ºC      ")
    soma=soma+f
print("A soma dos valores Fahreinheint: ",soma)
